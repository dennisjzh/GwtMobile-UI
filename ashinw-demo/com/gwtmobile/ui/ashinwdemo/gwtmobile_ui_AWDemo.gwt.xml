<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.4.0.beta//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.4.0.beta/distro-source/core/src/gwt-module.dtd">
<module rename-to="awdemo">
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.gwtmobile.ui.gwtmobile_ui" />
	<entry-point class="com.gwtmobile.ui.ashinwdemo.client.DemoApp" />

	<set-property name="user.agent" value="safari" />

	<replace-with class="com.gwtmobile.ui.ashinwdemo.client.mobile.MobileClientFactoryImpl">
		<when-type-is class="com.gwtmobile.ui.ashinwdemo.client.ClientFactory" />
	</replace-with>

	<replace-with
		class="com.gwtmobile.ui.client.page.BrowserHistoryNavigationImpl">
		<when-type-is
			class="com.gwtmobile.ui.client.page.SerialHistoryNavigationImpl" />
	</replace-with>

	<source path="client" />
</module>